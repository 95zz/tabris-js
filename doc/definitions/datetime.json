{
  "id": "rwt.widgets.DateTime",
  "type": "object",
  "extends": "rwt.widgets.Composite",
  "properties": {
    "style": {
      "description": "Valid values are 'DATE' and 'TIME'.",
      "type": "array of strings"
    },
    "year": {
      "type": "number"
    },
    "month": {
      "type": "number"
    },
    "day": {
      "type": "number"
    },
    "minutes": {
      "type": "number"
    },
    "seconds": {
      "type": "number"
    },
    "datePattern": {
      "type": "string"
    },
    "dateSeparator": {
      "type": "string"
    },
    "toolTip": {
      "type": "string"
    }
  },
  "listeners": [
    "selection"
  ]
}
