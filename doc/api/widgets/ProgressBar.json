{
  "type": "ProgressBar",
  "description": "A widget representing a numeric value as a horizontal bar with a growing indicator.",
  "include": ["Widget"],
  "properties": {
    "minimum": {
      "type": "number",
      "default": "0"
    },
    "maximum": {
      "type": "number",
      "default": "100"
    },
    "selection": {
      "type": "number",
      "default": "0"
    },
    "state": {
      "type": "string",
      "values": ["normal", "paused", "error"],
      "default": "normal"
    }
  },
  "links": [
    {
      "title" : "Simple ProgressBar snippet",
      "path" : "https://github.com/eclipsesource/tabris-js/blob/master/snippets/progressbar/progressbar.js"
    }
  ]
}
