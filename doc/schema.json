          "CHECK",
          "RADIO",
          "FLAT",
          "TOGGLE"
        ]
      },
      "text": {
        "type": "string"
      },
      "alignment": {
        "type": "string",
        "values": [
          "left",
          "right",
          "center"
        ]
      },
      "selection": {
        "type": "boolean"
      },
      "image": {
        "type": "array",
        "items": [
          {
            "description": "Image Path",
            "type": "string"
          },
          {
            "description": "width",
            "type": "number"
          },
          {
            "description": "height",
            "type": "number"
          }
        ]
      }
    },
    "font": {
      "type": "array",
      "items": [
        {
          "type": "array",
          "items": [
            {
              "description": "Font Name",
              "type": "string"
            }
          ]
        },
        {
          "description": "Font Size",
          "type": "number"
        },
        {
          "description": "Bold",
          "type": "boolean"
        },
        {
          "description": "Italic",
          "type": "boolean"
        }
      ]
    }
  },
  "listeners": [
    "Selection"
  ]
}